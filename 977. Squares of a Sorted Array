import java.util.*;
class Solution {
    public int[] sortedSquares(int[] nums) {
        for(int i=0;i<nums.length;i++){
            nums[i]=nums[i]*nums[i];
        }
        Arrays.sort(nums);
        // for(int i=0;i<nums.length-1;i++){
        //     int temp=i;
        //     for(int j=i+1;j<nums.length;j++){
        //         if(nums[temp]>nums[j]){
        //             temp=j;
        //         }
        //     }
        //     int t=nums[i];
        //     nums[i]=nums[temp];
        //     nums[temp]=t;
        // }
        return nums;
    }
}
